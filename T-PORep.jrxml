	<parameter name="PoNumber" class="java.lang.String"/>
	<parameter name="RevNumber" class="java.lang.Float"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    purchaseorders.Id,
    purchaseorders.PONumber,
    DATE_FORMAT(purchaseorders.PODate,'%d-%b-%Y' ) AS PODate,
    DATE_FORMAT(purchaseorders.QuotationDate, '%d-%b-%Y' ) AS QuotationDate,
    purchaseorders.RevisionNumber,
    purchaseorders.ProcurementType,
    purchaseorders.Currency,
    purchaseorders.VendorName,
    purchaseorders.VendorId,
    purchaseorders.QuotationReferenceNumber,
    purchaseorders.VendorAddress,
    purchaseorders.DeliveryTerms,
    purchaseorders.PaymentTerms,
    purchaseorders.ShippingMode,
    purchaseorders.ShipTo,
    purchaseorders.BillTo,
    purchaseorders.RetentionPeriod,
    purchaseorders.SpecialTermsAndConditions,
    purchaseorders.TotalAmount,
    purchaseorders.Unit,
    purchaseorders.CreatedBy,
    purchaseorders.CreatedOn,
    vendormasters.GST as vendorGst,
    companyaddresse.GSTNNumber as compGst,
    poitems.Id,
    poitems.ItemNumber,
    poitems.MftrItemNumber,
    poitems.Description as des,
    poitems.UOM,
    poitems.UnitPrice,
    poitems.SpecialInstruction,
    poitems.Qty,
    poitems.BalanceQty,
    poitems.SubTotal,
   ROUND( CASE 
        WHEN purchaseorders.Currency IN ('INR', 'inr') THEN poitems.SubTotal * 1.18
        ELSE poitems.SubTotal
    END) AS TotalValueWithtax,
    poitems.CGST,
    poitems.SGST,
    poitems.IGST,
    poitems.UTGST,
    poitems.TotalWithTax,
    vendormasters.Website,
    contact.Email,
    contact.MobileNumber,
    (SELECT ROUND(SUM(CASE 
                    WHEN purchaseorders.Currency IN ('INR', 'inr') THEN poitems.SubTotal * 1.18
                    ELSE poitems.SubTotal
                END))
    FROM tras_getapcs_purchase.poitems
    WHERE tras_getapcs_purchase.poitems.PurchaseOrderId = purchaseorders.Id) AS TotalValueWithtaxSum
FROM tras_getapcs_purchase.purchaseorders
LEFT JOIN tras_getapcs_purchase.poitems
    ON tras_getapcs_purchase.purchaseorders.Id = tras_getapcs_purchase.poitems.PurchaseOrderId      
LEFT JOIN tras_getapcs_master.vendormasters
    ON tras_getapcs_master.vendormasters.VendorId = tras_getapcs_purchase.purchaseorders.VendorId
LEFT JOIN tras_getapcs_master.contact
    ON tras_getapcs_master.vendormasters.VendorId = tras_getapcs_master.contact.Id
LEFT JOIN tras_getapcs_master.companyaddresse  
    ON tras_getapcs_master.companyaddresse.POAddress = tras_getapcs_purchase.purchaseorders.ShipTo
WHERE 
    tras_getapcs_purchase.purchaseorders.PONumber = $P{PoNumber}
    AND tras_getapcs_purchase.purchaseorders.RevisionNumber = $P{RevNumber}
GROUP BY 
    purchaseorders.Id,
    purchaseorders.PONumber,
    PODate,
    QuotationDate,
    purchaseorders.RevisionNumber,
    purchaseorders.ProcurementType,
    purchaseorders.Currency,
    purchaseorders.VendorName,
    purchaseorders.VendorId,
    purchaseorders.QuotationReferenceNumber,
    purchaseorders.VendorAddress,
    purchaseorders.DeliveryTerms,
    purchaseorders.PaymentTerms,
    purchaseorders.ShippingMode,
    purchaseorders.ShipTo,
    purchaseorders.BillTo,
    purchaseorders.RetentionPeriod,
    purchaseorders.SpecialTermsAndConditions,
    purchaseorders.TotalAmount,
    purchaseorders.Unit,
    purchaseorders.CreatedBy,
    purchaseorders.CreatedOn,
    vendorGst,
    compGst,
    poitems.Id,
    poitems.ItemNumber,
    poitems.MftrItemNumber,
    des,
    poitems.UOM,
    poitems.UnitPrice,
    poitems.Qty,
    poitems.SpecialInstruction,
    poitems.BalanceQty,
    poitems.SubTotal,
    poitems.CGST,
    poitems.SGST,
    poitems.IGST,
    poitems.UTGST,
    poitems.TotalWithTax,
    vendormasters.Website,
    contact.Email,
    contact.MobileNumber]]>
	</queryString>
