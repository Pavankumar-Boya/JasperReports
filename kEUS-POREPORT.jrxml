	<queryString language="SQL">
		<![CDATA[SELECT purchaseorders.Id,
	purchaseorders.PONumber,	
	DATE_FORMAT( purchaseorders.PODate,'%d-%b-%Y') AS PODate,	
	DATE_FORMAT(purchaseorders.QuotationDate,'%d-%b-%Y') AS QuotationDate,
	purchaseorders.RevisionNumber,	
	purchaseorders.ProcurementType,	
	purchaseorders.Currency,	
	purchaseorders.VendorName,	
	purchaseorders.AmountInWords,
	purchaseorders.VendorId,	
	purchaseorders.QuotationReferenceNumber,
	purchaseorders.VendorAddress,	
	purchaseorders.DeliveryTerms,	
	purchaseorders.PaymentTerms,	
	purchaseorders.ShippingMode,	
	purchaseorders.ShipTo,	
	purchaseorders.BillTo,	
	purchaseorders.RetentionPeriod,	
	purchaseorders.SpecialTermsAndConditions,
	purchaseorders.TotalAmount,	
	purchaseorders.Unit,	
	purchaseorders.CreatedBy,	
	purchaseorders.CreatedOn,	
	DATE_FORMAT(purchaseorders.LastModifiedOn,'%d-%b-%Y') AS LastModifiedOn,
	purchaseorders.LastModifiedBy,	
    purchaseorders.SpecialTermsAndConditions,
        poitems.ItemNumber,
        poitems.MftrItemNumber,
        poitems.Description,
        poitems.UOM,
        poitems.UnitPrice,
        poitems.PartType,
        poitems.Qty,
        poitems.SubTotal,
        poitems.TotalWithTax,
        poitems.IGST,
        poitems.CGST,
        poitems.SGST,
        poitems.UTGST,
        itemmasters.ItemNumber,
        itemmasters.Hsn,
        itemmasters.RemarksToVendor,
        companyaddresse.POAddress,
		vendormasters.VendorId as VId,
		 vendormasters.VendorName,		
 vendormasters.Address,
 vendormasters.City,
 vendormasters.State,
 vendormasters.Country,	
 vendormasters.PinZipCode,	
 vendormasters.ShippingMode	,					
 vendormasters.PaymentTerms,		
 vendormasters.SpecialTerms	,
 vendormasters.PreferredFreightForwader,				
 vendormasters.GST,		
 vendormasters.PAN,
 addresses.POAddress as VAdd,
addresses.GSTNNumber as VGst,
addresses.PANNumber as VPan,
		getapcs_keus_purchase.poadddeliveryschedules.`Id`,
		DATE_FORMAT( poadddeliveryschedules.PODeliveryDate,'%d-%b-%Y') AS PODeliveryDate,
		getapcs_keus_purchase.poadddeliveryschedules.`PODeliveryQty`,
		getapcs_keus_purchase.poadddeliveryschedules.`POItemDetailId`,
		getapcs_keus_purchase.poitems.`ItemNumber`,
(poitems.Qty * poitems.UnitPrice) AS SubTotal,
IFNULL(FORMAT(COALESCE(poitems.CGST, '') / 100 * COALESCE(poitems.SubTotal, ''), 2), '') AS AMTCGST,
IFNULL(FORMAT(COALESCE(poitems.SGST, '') / 100 * COALESCE(poitems.SubTotal, ''), 2), '') AS AMTSGST,
IFNULL(FORMAT(COALESCE(poitems.IGST, '') / 100 * COALESCE(poitems.SubTotal, ''), 2), '') AS AMTIGST
	 FROM getapcs_keus_purchase.purchaseorders
	LEFT  JOIN getapcs_keus_purchase.poitems 
	 ON  getapcs_keus_purchase.purchaseorders.`Id` = getapcs_keus_purchase.poitems.`PurchaseOrderId`      
	  LEFT JOIN  getapcs_keus_purchase.poadddeliveryschedules 
	 ON   getapcs_keus_purchase.poitems.`Id` =  getapcs_keus_purchase.poadddeliveryschedules.`POItemDetailId` 
	LEFT  JOIN getapcs_keus_master.vendormasters
	  ON getapcs_keus_master.vendormasters.`Id` = getapcs_keus_purchase.purchaseorders.`VendorId`
	 LEFT JOIN getapcs_keus_master.itemmasters 
	 ON getapcs_keus_master.itemmasters.`ItemNumber` = getapcs_keus_purchase.poitems.`ItemNumber`
LEFT JOIN getapcs_keus_master.companyaddresse 
ON getapcs_keus_master.companyaddresse.`POAddress` = getapcs_keus_purchase.purchaseorders.`ShipTo` 
   AND getapcs_keus_master.companyaddresse.`POAddress` = getapcs_keus_purchase.purchaseorders.`BillTo`
   LEFT  JOIN getapcs_keus_master.addresses
	  ON getapcs_keus_master.vendormasters.`Id` = getapcs_keus_master.addresses.`VendorMasterId`

	WHERE 	 getapcs_keus_purchase.purchaseorders.`PONumber` =$P{PONumber}
	AND  getapcs_keus_purchase.purchaseorders.`RevisionNumber` =$P{RevisionNumber}
	order by poitems.Id asc]]>
